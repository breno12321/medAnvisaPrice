{
  "openapi": "3.0.0",
  "info": {
    "title": "Anvisa Medication price",
    "version": "1.0",
    "description": "<img src=\"https://csp.qualisaude.telessaude.ufrn.br/static/images/logo_anvisa_topo.png\" />\nSimple API for fetching Anvisa medications and their respective price.\n<br/>\nOBS Every request has 1hour client side caching\n<br/>\n<strong>THIS API IS NOT OFFICIAL<strong/>\n"
  },
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "paths": {
    "/medication": {
      "get": {
        "summary": "Route for getting filtered medication requests",
        "tags": [
          "Medication"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "filter",
            "schema": {
              "type": "string",
              "enum": [
                "SUBSTÂNCIA",
                "CNPJ",
                "LABORATÓRIO",
                "CÓDIGO GGREM",
                "REGISTRO",
                "EAN 1",
                "EAN 2",
                "EAN 3",
                "PRODUTO",
                "APRESENTAÇÃO",
                "CLASSE TERAPÊUTICA",
                "TIPO DE PRODUTO (STATUS DO PRODUTO)",
                "REGIME DE PREÇO",
                "PF Sem Impostos",
                "PF 0%",
                "PF 12%",
                "PF 17%",
                "PF 17% ALC",
                "PF 17,5%",
                "PF 17,5% ALC",
                "PF 18%",
                "PF 18% ALC",
                "PF 20%",
                "PMC 0%",
                "PMC 12%",
                "PMC 17%",
                "PMC 17% ALC",
                "PMC 17,5%",
                "PMC 17,5% ALC",
                "PMC 18%",
                "PMC 18% ALC",
                "PMC 20%",
                "RESTRIÇÃO HOSPITALAR",
                "CAP",
                "CONFAZ 87",
                "ICMS 0%",
                "ANÁLISE RECURSAL",
                "LISTA DE CONCESSÃO DE CRÉDITO TRIBUTÁRIO (PIS/COFINS)",
                "COMERCIALIZAÇÃO 2019"
              ]
            },
            "description": "Pass the filter parameter you want to filter by"
          },
          {
            "in": "query",
            "name": "value",
            "description": "The value for the filter",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/MedicationResponse"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Medication": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "600f0a6eacb8baee443527b9"
          },
          "SUBSTÂNCIA": {
