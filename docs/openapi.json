      }
    },
    "responses": {
      "MedicationResponse": {
        "description": "Success response from medication API",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "ok"
                },
                "rows": {
                  "type": "integer",
                  "example": 2,
                  "description": "Number of rows that matched the search"
                },
                "data": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Medication"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
